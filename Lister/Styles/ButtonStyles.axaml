<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Design.PreviewWith>
    <Border Padding="20">
      <!-- Add Controls for Previewer Here -->
    </Border>
  </Design.PreviewWith>

  <!-- Add Styles Here -->

	<Style Selector="Button /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="FontSize" Value="16"/>
		<Setter Property="Height" Value="32"/>
	</Style>


	<Style Selector="Button.transparent /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="FontFamily" Value="{StaticResource FA}"/>
		<Setter Property="Foreground" Value="#1C547D"/>
		<Setter Property="Background" Value="#EEEEEE"/>
		<Setter Property="BorderThickness" Value="2"/>
	</Style>
	<Style Selector="Button.transparent:pointerover /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#1C547D"/>
		<Setter Property="BorderBrush" Value="#4AACFE"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>
	<Style Selector="Button.transparent:focus /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#2570A7"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>
	<Style Selector="Button.transparent:pressed /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#2570A7"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>
	<Style Selector="Button.transparent:disabled /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#DBCAAE"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>

	
	<Style Selector="Button.extender /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Width" Value="26"/>
		<Setter Property="Height" Value="26"/>
		<Setter Property="FontFamily" Value="{StaticResource FA}"/>
		<Setter Property="Foreground" Value="#1C547D"/>
		<Setter Property="Background" Value="#eeeeee"/>
		<Setter Property="BorderThickness" Value="2"/>
	</Style>
	<Style Selector="Button.extender:pointerover /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#1C547D"/>
		<Setter Property="BorderBrush" Value="#4AACFE"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>
	<Style Selector="Button.extender:focus /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#2570A7"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>
	<Style Selector="Button.extender:pressed /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#2570A7"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>
	<Style Selector="Button.extender:disabled /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#DBCAAE"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>


	<Style Selector="Button.littlePicture /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="FontFamily" Value="{StaticResource FA}"/>
		<Setter Property="Foreground" Value="#1C547D"/>
		<Setter Property="Background" Value="#EEEEEE"/>
		<Setter Property="FontSize" Value="20"/>
		<Setter Property="BorderThickness" Value="2"/>
	</Style>
	<Style Selector="Button.littlePicture:pointerover /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#1C547D"/>
		<Setter Property="BorderBrush" Value="#4AACFE"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>
	<Style Selector="Button.littlePicture:focus /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#2570A7"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>
	<Style Selector="Button.littlePicture:pressed /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#2570A7"/>
		<Setter Property="Background" Value="#EEEEEE"/>
		<Setter Property="Padding" Value="8,5"/>
	</Style>
	<Style Selector="Button.littlePicture:disabled /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#DBCAAE"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>


	<Style Selector="Button.upDownArrows /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="FontFamily" Value="{StaticResource FA}"/>
		<Setter Property="Foreground" Value="#1C547D"/>
		<Setter Property="Background" Value="#EEEEEE"/>
		<Setter Property="FontSize" Value="20"/>
		<Setter Property="Width" Value="26"/>
		<Setter Property="Height" Value="26"/>
		<Setter Property="Padding" Value="5,2"/>
	</Style>
	<Style Selector="Button.upDownArrows:pointerover /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#1C547D"/>
		<Setter Property="BorderBrush" Value="#4AACFE"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>
	<Style Selector="Button.upDownArrows:focus /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#2570A7"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>
	<Style Selector="Button.upDownArrows:pressed /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#2570A7"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>
	<Style Selector="Button.upDownArrows:disabled /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#DBCAAE"/>
		<Setter Property="Background" Value="#EEEEEE"/>
	</Style>


	<Style Selector="Button.Primary /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#FAFAFA"/>
		<Setter Property="Background" Value="#1C547D"/>
		<Setter Property="BorderThickness" Value="2"/>
	</Style>
	<Style Selector="Button.Primary:pointerover /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#FAFAFA"/>
		<Setter Property="Background" Value="#1C547D"/>
		<Setter Property="BorderBrush" Value="#4AACFE"/>
	</Style>
	<Style Selector="Button.Primary:focus /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#FAFAFA"/>
		<Setter Property="Background" Value="#2570A7"/>
	</Style>
	<Style Selector="Button.Primary:pressed /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#FAFAFA"/>
		<Setter Property="Background" Value="#94BEE6"/>
	</Style>
	<Style Selector="Button.Primary:disabled /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Foreground" Value="#DBCAAE"/>
		<Setter Property="Background" Value="#FCF2E3"/>
	</Style>



	<Style Selector="Button.Switcher /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Background" Value="#1C547D"/>
		<Setter Property="BorderThickness" Value="2"/>
	</Style>
	<Style Selector="Button.Switcher:pointerover /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Background" Value="#1C547D"/>
		<Setter Property="BorderBrush" Value="#4AACFE"/>
	</Style>
	<Style Selector="Button.Switcher:focus /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Background" Value="#2570A7"/>
	</Style>
	<Style Selector="Button.Switcher:pressed /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Background" Value="#94BEE6"/>
	</Style>
	
	
	<Style Selector="Button.scrollButton /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="FontSize" Value="9"/>
	</Style>
	<Style Selector="Button.scrollButton /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Height" Value="15"/>
	</Style>
	<Style Selector="Button.scrollButton /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="FontFamily" Value="{StaticResource FA}"/>
	</Style>

	
	<Style Selector="Button.picture /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="FontFamily" Value="{StaticResource FA}"/>
	</Style>

	
	
	<Style Selector="Button.superButton /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Background" Value="#4AACFE"/>
		<Setter Property="BorderThickness" Value="2"/>
		<Setter Property="Foreground" Value="#FAFAFA"/>
	</Style>
	<Style Selector="Button.superButton:pointerover /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Background" Value="#4AACFE"/>
		<Setter Property="Foreground" Value="#FAFAFA"/>
		<Setter Property="BorderBrush" Value="#2570A7"/>
		<Setter Property="Padding" Value="6,3"/>
	</Style>
	<Style Selector="Button.superButton:focus /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Background" Value="#94BEE6"/>
		<Setter Property="Foreground" Value="#FAFAFA"/>
		<Setter Property="Padding" Value="7,3"/>
	</Style>
	<Style Selector="Button.superButton:pressed /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Background" Value="#D5E8F6"/>
		<Setter Property="Foreground" Value="#FAFAFA"/>
	</Style>
	<Style Selector="Button.superButton:disabled /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Background" Value="#FCF2E3"/>
		<Setter Property="Foreground" Value="#DBCAAE"/>
	</Style>


	<Style Selector="Button.modalButton /template/ ContentPresenter#PART_ContentPresenter">
		<Setter Property="Width" Value="80"/>
		<Setter Property="Height" Value="24"/>
		<Setter Property="FontSize" Value="11"/>
		<Setter Property="BorderThickness" Value="2"/>
		<Setter Property="BorderBrush" Value="Gray"/>
		<Setter Property="CornerRadius" Value="0"/>
	</Style>
	
</Styles>


